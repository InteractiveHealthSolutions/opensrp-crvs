        <input type="date" name="/crvs_pregnancy_notification/lmp_date" required="required" data-constraint=".&lt;= /crvs_pregnancy_notification/today " data-type-xml="date"/>
    </label>
    <fieldset class="question ">
        <fieldset>
            <legend>
                <span lang="" class="question-label active">EDD: 
                    <span class="or-output" data-value=" /crvs_pregnancy_notification/edd_formatted "></span>
                </span>
            </legend>
            <div class="option-wrapper">
                <label>
                    <input value="OK" type="radio" name="/crvs_pregnancy_notification/edd_note" data-type-xml="string"/>
                    <span class="option-label active" lang="">OK</span>
                </label>
            </div>
        </fieldset>
    </fieldset>
    <label class="question non-select ">
        <span lang="" class="question-label active">Planned facility for delivery?</span>
        <span class="required">*</span>
        <input type="text" name="/crvs_pregnancy_notification/planned_facility_delivery" required="required" data-type-xml="string"/>
    </label>
    <label class="question non-select ">
        <span lang="" class="question-label active">Planned mode of transportation for delivery?</span>
        <span class="required">*</span>
        <input type="text" name="/crvs_pregnancy_notification/planned_transportation_delivery" required="required" data-type-xml="string"/>
    </label>
    <fieldset class="question ">
        <fieldset>
            <legend>
                <span lang="" class="question-label active">Is pregnant woman currently enrolled in HIV care?</span>
                <span class="required">*</span>
            </legend>
            <div class="option-wrapper">
                <label class="">
                    <input type="radio" name="/crvs_pregnancy_notification/enrolled_hiv_care" data-name="/crvs_pregnancy_notification/enrolled_hiv_care" value="1065" required="required" data-type-xml="select1"/>
                    <span lang="" class="option-label active">Yes</span>
                </label>
                <label class="">
                    <input type="radio" name="/crvs_pregnancy_notification/enrolled_hiv_care" data-name="/crvs_pregnancy_notification/enrolled_hiv_care" value="1066" required="required" data-type-xml="select1"/>
                    <span lang="" class="option-label active">No</span>
                </label>
            </div>
        </fieldset>
    </fieldset>
    <label class="question non-select ">
        <span lang="" class="or-constraint-msg active">Enter values 1 to 20 only. Include current pregnancy in the count.</span>
        <span lang="" class="question-label active">Number of times woman has been pregnant</span>
        <span class="required">*</span>
        <span lang="" class="or-hint active">(Including current pregnancy)</span>
        <input type="number" name="/crvs_pregnancy_notification/gravida" required="required" data-constraint=".&gt; 0 and .&lt;= 20" data-type-xml="int"/>
    </label>
    <label class="question or-branch pre-init non-select ">
        <span lang="" class="or-constraint-msg active">Births must be less than number of previous pregnancies plus current pregnancy.</span>
        <span lang="" class="question-label active">Number of times woman has given birth</span>
        <span class="required">*</span>
        <span lang="" class="or-hint active">(Including any still births)</span>
        <input type="number" name="/crvs_pregnancy_notification/parity" required="required" data-constraint=".&lt;  /crvs_pregnancy_notification/gravida " data-relevant=" /crvs_pregnancy_notification/gravida  &gt; 1" data-type-xml="int"/>
    </label>
    <fieldset class="question ">
        <fieldset>
            <legend>
                <span lang="" class="question-label active">Does woman have any of the following chronic medical conditions?</span>
                <span class="required">*</span>
            </legend>
            <div class="option-wrapper">
                <label class="">
                    <input type="checkbox" name="/crvs_pregnancy_notification/medical_histories" value="tb" required="required" data-type-xml="select"/>
                    <span lang="" class="option-label active">TB</span>
                </label>
                <label class="">
                    <input type="checkbox" name="/crvs_pregnancy_notification/medical_histories" value="hypertension" required="required" data-type-xml="select"/>
                    <span lang="" class="option-label active">Hypertension</span>
                </label>
                <label class="">
                    <input type="checkbox" name="/crvs_pregnancy_notification/medical_histories" value="heart_diseases" required="required" data-type-xml="select"/>
                    <span lang="" class="option-label active">Heart-related diseases</span>
                </label>
                <label class="">
                    <input type="checkbox" name="/crvs_pregnancy_notification/medical_histories" value="diabetes" required="required" data-type-xml="select"/>
                    <span lang="" class="option-label active">Diabetes</span>
                </label>
            </div>
        </fieldset>
    </fieldset>
    <label class="question or-appearance-minimal ">
        <span lang="" class="question-label active">Woman's blood group</span>
        <span class="required">*</span>
        <select name="/crvs_pregnancy_notification/blood_group" data-name="/crvs_pregnancy_notification/blood_group" required="required" data-type-xml="select1">
            <option value="">...</option>
            <option value="690">A+</option>
            <option value="692">A-</option>
            <option value="694">B+</option>
            <option value="696">B-</option>
            <option value="1230">AB+</option>
            <option value="1231">AB-</option>
            <option value="699">O+</option>
            <option value="701">O-</option>
        </select>
        <span class="or-option-translations" style="display:none;"></span>
    </label>
    <label class="question non-select ">
        <span lang="" class="question-label active">Woman's height (in cms)</span>
        <span class="required">*</span>
        <input type="number" name="/crvs_pregnancy_notification/height" required="required" data-type-xml="int"/>
    </label>
    <label class="question non-select ">
        <span lang="" class="question-label active">Woman's weight at start of pregnancy (in kgs)</span>
        <span class="required">*</span>
        <input type="number" name="/crvs_pregnancy_notification/weight_start_pregnancy" required="required" data-type-xml="int"/>
    </label>
    <fieldset id="or-preload-items" style="display:none;">
        <label class="calculation non-select ">
            <input type="hidden" name="/crvs_pregnancy_notification/start" data-preload="timestamp" data-preload-params="start" data-type-xml="dateTime"/>
        </label>
        <label class="calculation non-select ">
            <input type="hidden" name="/crvs_pregnancy_notification/end" data-preload="timestamp" data-preload-params="end" data-type-xml="dateTime"/>
        </label>
        <label class="calculation non-select ">
            <input type="hidden" name="/crvs_pregnancy_notification/today" data-preload="date" data-preload-params="today" data-type-xml="date"/>
        </label>
        <label class="calculation non-select ">
            <input type="hidden" name="/crvs_pregnancy_notification/deviceid" data-preload="property" data-preload-params="deviceid" data-type-xml="string"/>
        </label>
        <label class="calculation non-select ">
            <input type="hidden" name="/crvs_pregnancy_notification/subscriberid" data-preload="property" data-preload-params="subscriberid" data-type-xml="string"/>
        </label>
    </fieldset>
    <fieldset id="or-calculated-items" style="display:none;">
        <label class="calculation non-select ">
            <input type="hidden" name="/crvs_pregnancy_notification/uc_name_display" data-calculate="if( /crvs_pregnancy_notification/phc_identifier ='uc','',concat('UC: ', /crvs_pregnancy_notification/phc_name ))" data-type-xml="string"/>
        </label>
        <label class="calculation non-select ">
            <input type="hidden" name="/crvs_pregnancy_notification/address_encounter_center_type" data-calculate="if( /crvs_pregnancy_notification/phc_identifier ='uc','uc','hospital')" data-type-xml="string"/>
        </label>
        <label class="calculation non-select ">
            <input type="hidden" name="/crvs_pregnancy_notification/address_encounter_uc" data-calculate="if( /crvs_pregnancy_notification/phc_identifier  != 'uc',  /crvs_pregnancy_notification/phc_name ,  /crvs_pregnancy_notification/address_encounter_center )" data-type-xml="string"/>
        </label>
        <label class="calculation non-select ">
            <input type="hidden" name="/crvs_pregnancy_notification/edd" data-calculate="date( /crvs_pregnancy_notification/lmp_date +280)" data-type-xml="string"/>
        </label>
        <label class="calculation non-select ">
            <input type="hidden" name="/crvs_pregnancy_notification/edd_formatted" data-calculate="format-date( /crvs_pregnancy_notification/edd , '%Y-%n-%e')" data-type-xml="string"/>
        </label>
        <label class="calculation non-select ">
            <input type="hidden" name="/crvs_pregnancy_notification/meta/instanceID" data-calculate="concat('uuid:', uuid())" data-type-xml="string"/>
        </label>
        <label class="calculation non-select ">
            <input type="hidden" name="/crvs_pregnancy_notification/formhub/uuid" data-calculate="'1b20fd97847845f29154b3ad6a19202c'" data-type-xml="string"/>
        </label>
    </fieldset>
</form>